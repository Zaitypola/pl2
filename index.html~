       <script language="JavaScript">
        function selecturl(s) 
{
  var gourl = s.options[s.selectedIndex].value;
  if ((s.selectedIndex!=0) &amp;&amp; ((gourl != null) &amp;&amp; (gourl != "") ))
  {
   window.top.location.href = gourl;
  }
}
